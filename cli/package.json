{
  "name": "cli",
  "description": "The world's lightest static-site generator",
  "version": "1.0.0",
  "author": "Shailen Naidoo",
  "bin": {
    "hydrogen": "./bin/run"
  },
  "bugs": "https://github.com/ShailenNaidoo/hydrogen/issues",
  "dependencies": {
    "@oclif/command": "^1",
    "@oclif/config": "^1",
    "@oclif/plugin-help": "^2",
    "tslib": "^1"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1",
    "@types/node": "^10",
    "globby": "^10",
    "oclif": "^1.13.5",
    "ts-node": "^8",
    "typescript": "^3.3"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ShailenNaidoo/hydrogen",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "hydrogen",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ShailenNaidoo/hydrogen",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json",
    "prepack": "rm -rf lib && tsc -b && oclif-dev manifest && oclif-dev readme",
    "test": "echo NO TESTS",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
